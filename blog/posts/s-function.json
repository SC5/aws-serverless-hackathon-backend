{
  "name": "posts",
  "runtime": "nodejs4.3",
  "description": "Lambda Function for ${project} - ${stage}",
  "customName": false,
  "customRole": false,
  "handler": "posts/handler.handler",
  "timeout": 30,
  "memorySize": 512,
  "authorizer": {},
  "custom": {
    "excludePatterns": [],
    "envVars": [],
    "cors": { 
      "allowOrigin": "*", 
      "allowHeaders": [
        "Content-Type", 
        "X-Amz-Date", 
        "Authorization",
        "X-Api-Key"
      ] 
    }
  },
  "endpoints": [
